# This is Git's per-user configuration file.
[user]
	name = Michael Rogov
	email = michael@e8storage.com
[core]
  excludesfile = /Users/michaelr/.gitignore_global
  editor = /usr/bin/vim
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
	d = difftool
[push]
	default = simple
[diff]
	tool = vimdiff
[pull]
	rebase = true
[difftool]
	prompt = false
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
